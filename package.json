{
    "name": "rrze-newsletter",
    "version": "2.4.0",
    "description": "Plugin for creating and sending HTML Newsletters.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/RRZE-Webteam/rrze-newsletter.git"
    },
    "license": "GPL-2.0-or-later",
    "bugs": {
        "url": "https://github.com/RRZE-Webteam/rrze-newsletter/issues"
    },
    "homepage": "https://github.com/RRZE-Webteam/rrze-newsletter#readme",
    "engines": {
        "node": ">=21.2.0",
        "npm": ">=10.2.3"
    },
    "devDependencies": {
        "@wordpress/icons": "latest",
        "@wordpress/scripts": "latest",
        "classnames": "latest",
        "mjml-browser": "latest",
        "ts-loader": "latest",
        "typescript": "latest"
    },
    "browserslist": [
        "extends @wordpress/browserslist-config"
